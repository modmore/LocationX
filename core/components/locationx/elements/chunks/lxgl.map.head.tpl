<script type="text/javascript">
    lx.data['[[+id]]'] = {
        markers: [[+data]],
        markerObjs: [],
        mapOptions: {
            center: new google.maps.LatLng([[+center_lat]], [[+center_lng]]),
            zoom: 10,
            mapTypeId: google.maps.MapTypeId.ROADMAP
        },
        map: null
    };
    lx.initMap('[[+id]]');
</script>
